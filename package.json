    "build-prod": "ng build -prod --aot=false --sourcemaps --verbose --extract-css --output-hashing=none",
    "build-app-prod": "npm run build-prod -- --app=app --base-href=/44fz/rd/app/",
    "build-fk-prod": "npm run build-prod -- --app=fk-app --base-href=/44fz/rd/fk-app/",
